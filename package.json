{
  "name": "megumi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:next 'gen:sass-type --watch'",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint-styles": "stylelint 'src/**/*.scss'",
    "lint-styles:fix": "stylelint 'src/**/*.scss' --fix",
    "lint:fix": "eslint --fix src",
    "type-check": "tsc --noEmit",
    "fix": "pnpm run lint:fix && pnpm run lint-styles:fix && pnpm run format && pnpm run type-check",
    "gen": "run-p gen:*",
    "gen:sass-type": "typed-scss-modules src",
    "gen:icons": "node scripts/gen-icons.mjs",
    "dl-icons": "node scripts/dl-icons.mjs",
    "postinstall": "pnpm run gen"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@svgr/babel-plugin-remove-jsx-attribute": "8.0.0",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "8.0.0",
    "@svgr/cli": "^8.0.1",
    "@svgr/plugin-jsx": "^8.0.1",
    "@types/gtag.js": "^0.0.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "normalize.css": "^8.0.1",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5",
    "zx": "^8.1.0"
  }
}
